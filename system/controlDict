FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     simpleFoam;

startFrom       startTime;
startTime       0;

endTime         200;          // breve corsa di prova
deltaT          1;

writeControl    timeStep;     // scrivi per numero di step
writeInterval   20;           // 20/, 40/, 60/, ...

purgeWrite      0;
writeFormat     ascii;
writePrecision  7;
writeCompression off;
timeFormat      general;
timePrecision   6;

runTimeModifiable yes;

